<?xml version="1.0"?>

<launch>

    <arg name="output" default="screen" />

    <!-- Start octomap node -->
    <node name="pepper_move_base_mapper" pkg="pepper_move_base_mapping" type="pepper_move_base_mapper" required="true" output="$(arg output)">
        <rosparam command="load" file="$(find pepper_move_base_mapping)/config/pepper_move_base_mapping.yaml" />
    </node>

</launch>